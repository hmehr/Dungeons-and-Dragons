    private void armorBtnController(JToggleButton pToggleButton)
    {
        if (pToggleButton != jToggleBtnLight)
        {
            jToggleBtnLight.setSelected(false);
            _light = false;


        }
        if (pToggleButton != jToggleBtnMedium)
        {
            jToggleBtnMedium.setSelected(false);
            _medium = false;


        }
        if (pToggleButton != jToggleBtnHeavy)
        {
            jToggleBtnHeavy.setSelected(false);
            _heavy = false;


        }
        if (pToggleButton != jToggleBtnShield)
        {
            jToggleBtnShield.setSelected(false);
            _shield = false;
        }
    }
