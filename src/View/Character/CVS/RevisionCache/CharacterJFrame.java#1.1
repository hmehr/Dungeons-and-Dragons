    {
        if (pCurrentButton != jButtonCh1)
            jButtonCh1.setBorder(null);
         if (pCurrentButton != jButtonCh2)
            jButtonCh2.setBorder(null);
         if (pCurrentButton != jButtonCh3)
            jButtonCh3.setBorder(null);
         if (pCurrentButton != jButtonCh4)
